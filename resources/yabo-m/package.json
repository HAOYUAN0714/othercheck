{
  "name": "vue-admin-template",
  "version": "4.2.1",
  "description": "A vue admin template with Element UI & axios & iconfont & permission control & lint",
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "env-cmd -f .env.yabo.development vue-cli-service serve",
    "dev:betball": "env-cmd -f .env.betball.development vue-cli-service serve",
    "dev:bibet": "env-cmd -f .env.bibet.development vue-cli-service serve",
    "dev:m6app": "env-cmd -f .env.m6app.development vue-cli-service serve",
    "dev:yabolive": "env-cmd -f .env.yabolive.development vue-cli-service serve",
    "dev:bblivem": "env-cmd -f .env.bblivem.development vue-cli-service serve",
    "build:prod": "env-cmd -f .env.yabo.production vue-cli-service build",
    "build:betball": "env-cmd -f .env.betball.production vue-cli-service build",
    "build:bibet": "env-cmd -f .env.bibet.production vue-cli-service build",
    "build:m6app": "env-cmd -f .env.m6app.production vue-cli-service build",
    "build:yabolive": "env-cmd -f .env.yabolive.production vue-cli-service build",
    "build:bblivem": "env-cmd -f .env.bblivem.production vue-cli-service build",
    "build:stage": "env-cmd -f .env.yabo.production vue-cli-service build --mode staging",
    "preview": "node build/index.js --preview",
    "lint": "eslint --ext .js,.vue src",
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "test:ci": "npm run lint && npm run test:unit",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/vue-fontawesome": "^0.1.8",
    "axios": "0.18.1",
    "dplayer": "^1.25.0",
    "element-ui": "2.7.2",
    "env-cmd": "^10.0.1",
    "flatpickr": "^4.6.3",
    "flv.js": "^1.5.0",
    "jquery": "^3.4.1",
    "js-cookie": "2.2.0",
    "mobile-detect": "^1.4.4",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "normalize.css": "7.0.0",
    "nprogress": "0.2.0",
    "path-to-regexp": "2.4.0",
    "video.js": "^7.6.6",
    "videojs-flash": "^2.2.1",
    "vue": "2.6.10",
    "vue-axios": "^2.1.5",
    "vue-clipboard2": "^0.3.1",
    "vue-cookie": "^1.1.4",
    "vue-flatpickr-component": "^8.1.5",
    "vue-i18n": "^8.15.1",
    "vue-material-design-icons": "^4.4.0",
    "vue-mobile-detection": "^1.0.0",
    "vue-router": "3.0.6",
    "vue-video-player": "^5.0.2",
    "vuejs-datepicker": "^1.6.2",
    "vuex": "3.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/register": "7.0.0",
    "@vue/cli-plugin-babel": "3.6.0",
    "@vue/cli-plugin-eslint": "^3.9.1",
    "@vue/cli-plugin-unit-jest": "3.6.3",
    "@vue/cli-service": "3.6.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "autoprefixer": "^9.5.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "chalk": "2.4.2",
    "connect": "3.6.6",
    "eslint": "5.15.3",
    "eslint-plugin-vue": "5.2.2",
    "html-webpack-plugin": "3.2.0",
    "mockjs": "1.0.1-beta3",
    "node-sass": "^4.9.0",
    "runjs": "^4.3.2",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "2.1.3",
    "script-loader": "0.7.2",
    "serve-static": "^1.13.2",
    "svg-sprite-loader": "4.1.3",
    "svgo": "1.2.2",
    "vue-template-compiler": "2.6.10"
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
